<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background_fsu_logo">

    <Button
        android:id="@+id/map"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="80dp"
        android:elevation="10dp"
        android:onClick="mapsClick"
        android:text="map"
        android:typeface="monospace"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:textSize="20dp"/>

    <Button
        android:id="@+id/schedule"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="62dp"
        android:elevation="10dp"
        android:onClick="scheduleClick"
        android:text="schedule"
        android:typeface="monospace"
        android:textSize="20dp"
        android:layout_below="@+id/map"
        android:layout_centerHorizontal="true" />

    <Button
        android:id="@+id/buildings"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/schedule"
        android:layout_below="@+id/schedule"
        android:layout_marginTop="64dp"
        android:elevation="10dp"
        android:onClick="buildingClick"
        android:text="buildings"
        android:typeface="monospace"
        android:textSize="20dp"/>

</RelativeLayout>